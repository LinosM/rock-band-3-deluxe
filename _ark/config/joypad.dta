   (wii_guitar_rb2 kControllerGuitar)
   (wii_roguitar kControllerGuitar)
   #endif
   (hx_midi_guitar_xbox kControllerRealGuitar)
   (hx_midi_keys_xbox $dx_joypad_type_keys)
   (hx_midi_guitar_ps3 kControllerRealGuitar)

   (wii_drums_rb2 wii_drums)
   (wii_roguitar wii_guitar)
   (wii_rodrums wii_rodrums)
   #endif
   (none joypad))
(instrument_mapping
   (ro_guitar $dx_joypad_type_guitar)
   (ro_guitar_xbox $dx_joypad_type_guitar)
   (strat_xbox $dx_joypad_type_guitar)
   (strat_xbox_rb2 $dx_joypad_type_guitar)
